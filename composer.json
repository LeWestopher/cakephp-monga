{
    "name": "lewestopher/cakephp-monga",
    "description": "CakeMonga plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "league/monga": "^1.2",
        "evert/phpdoc-md" : "~0.2.0",
        "mockery/mockery": "0.9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeMonga\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeMonga\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
